{
  "name": "satone",
  "type": "module",
  "version": "0.0.0",
  "description": "A fullstack framework that brings Solid and Elysia together.",
  "keywords": ["spa", "elysia", "solidjs", "vite", "api", "framework"],
  "author": "Mikkel ALMONTE--RINGAUD <contact@vexcited.com> (https://github.com/Vexcited)",
  "files": ["src", "package.json"],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "directory": "package",
    "url": "git+https://github.com/satone-js/satone.git"
  },
  "bin": {
    "satone": "./src/cli.ts"
  },
  "exports": {
    "./server": {
      "default": "./src/server/index.ts"
    },
    "./client": {
      "default": "./src/client/index.ts"
    },
    "./server/plugins/prod": {
      "default": "./src/server/plugins/prod.ts"
    }
  },
  "devDependencies": {
    "@types/babel__generator": "^7.27.0",
    "@types/bun": "^1.3.1",
    "solid-js": "^1.9.10",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@babel/generator": "^7.28.5",
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "@babel/types": "^7.28.5",
    "@elysiajs/eden": "^1.4.4",
    "@elysiajs/static": "^1.4.5",
    "@elysiajs/swagger": "^1.3.1",
    "elysia": "^1.4.13",
    "vite": "latest",
    "vite-plugin-pages": "^0.33.1",
    "vite-plugin-solid": "^2.11.10"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@latest"
  }
}
