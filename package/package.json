{
  "name": "satone",
  "type": "module",
  "version": "0.0.4",
  "description": "A fullstack framework that brings Solid and Elysia together.",
  "author": "Mikkel ALMONTE--RINGAUD <contact@vexcited.com> (https://github.com/Vexcited)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "directory": "package",
    "url": "git+https://github.com/satone-js/satone.git"
  },
  "bugs": {
    "url": "https://github.com/satone-js/satone/issues"
  },
  "keywords": [
    "spa",
    "elysia",
    "solidjs",
    "vite",
    "api",
    "framework"
  ],
  "sideEffects": false,
  "exports": {
    "./server": {
      "default": "./src/server/index.ts"
    },
    "./client": {
      "default": "./src/client/index.ts"
    },
    "./config": {
      "default": "./src/config/index.ts"
    },
    "./server/plugins/prod": {
      "default": "./src/server/plugins/prod.ts"
    }
  },
  "bin": {
    "satone": "src/cli.ts"
  },
  "files": [
    "package.json",
    "src"
  ],
  "dependencies": {
    "@babel/generator": "^7.28.5",
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "@babel/types": "^7.28.5",
    "@elysiajs/eden": "^1.4.6",
    "@elysiajs/static": "^1.4.7",
    "@elysiajs/swagger": "^1.3.1",
    "elysia": "^1.4.21",
    "vite": "7.3.1",
    "vite-plugin-pages": "^0.33.2",
    "vite-plugin-solid": "^2.11.10",
    "vite-tsconfig-paths": "^6.0.4"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.28.0",
    "@types/bun": "^1.3.5",
    "solid-js": "^1.9.10",
    "typescript": "^5.9.3"
  }
}
